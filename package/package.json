{
	"name": "react-tree-stream",
	"version": "0.0.3",
	"description": "Stream React trees recursively, LLM-style progressive rendering.",
	"keywords": [
		"react",
		"react-component",
		"react-library",
		"ui",
		"ui-component",
		"stream",
		"streaming",
		"text-stream",
		"tree-stream",
		"typewriter",
		"typewriter-effect",
		"typing",
		"typing-effect",
		"progressive",
		"progressive-rendering",
		"incremental-rendering",
		"sequential-rendering",
		"delayed-rendering",
		"gradual-reveal",
		"content-reveal",
		"character-by-character",
		"word-by-word",
		"nested",
		"recursive",
		"nested-components",
		"animation",
		"chat",
		"chat-ui",
		"assistant-ui",
		"llm",
		"ai",
		"openai",
		"chatgpt",
		"nextjs",
		"typescript"
	],
	"license": "MIT",
	"author": "Lindsay Spencer",
	"type": "module",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"sideEffects": false,
	"homepage": "https://github.com/lindsayjspencer/react-tree-stream#readme",
	"repository": {
		"type": "git",
		"url": "https://github.com/lindsayjspencer/react-tree-stream"
	},
	"scripts": {
		"build": "tsup src/index.tsx --format cjs,esm --dts --sourcemap --clean",
		"dev": "tsup src/index.tsx --format cjs,esm --dts --watch",
		"prepublishOnly": "pnpm build"
	},
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org"
	},
	"peerDependencies": {
		"react": ">=18.0.0"
	},
	"devDependencies": {
		"typescript": "^5.5.4",
		"tsup": "^8.1.0",
		"@types/react": "^18.3.3"
	}
}